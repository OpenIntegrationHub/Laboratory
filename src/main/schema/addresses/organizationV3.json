{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "$id": "https://github.com/openintegrationhub/Data-and-Domain-Models/blob/master/src/main/schema/addresses/organizationV3.json",
  "title": "Organization",
  "type": "object",
  "allOf": [
    {
      "$ref": "../oih-data-record.json"
    }
  ],
  "properties": {
    "name": {
      "type": "string",
      "description": "Name of the organization",
      "example": "Great Company"
    },
    "logo": {
      "type": "string",
      "description": "Logo of the organization",
      "example": "http://example.org/logo.png"
    },
    "contactData": {
      "type": "array",
      "items": {
        "$ref": "sharedDefinitionsV3.json#/definitions/ContactData"
      }
    },
    "categories": {
      "type": "array",
      "items": {
          "$ref": "sharedDefinitionsV3.json#/definitions/Category"
      }
    },
    "calendars": {
      "type": "array",
      "items": {
          "$ref": "sharedDefinitionsV3.json#/definitions/Calendar"
      }
    }
  }
}
